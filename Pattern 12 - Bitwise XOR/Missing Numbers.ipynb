    "def find_missing_number(arr):\n",
    "    n = len(arr)+1\n",
    "    \n",
    "    x1 = 1\n",
    "    for i in range(2,n+1):\n",
    "        x1 = x1^i\n",
    "\n",
    "    x2=arr[0]\n",
    "    for i in range(1,n-1):\n",
    "        x2 = x2 ^ arr[i]\n",
    "    return x1^x2"
   ]
