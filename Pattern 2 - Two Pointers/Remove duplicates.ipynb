   "source": [
    "def remove_duplicates(arr):\n",
    "    nextdup = 1\n",
    "    i = 1\n",
    "    while i < len(arr):\n",
    "        if arr[nextdup-1] != arr[i]:\n",
    "            arr[nextdup] = arr[i]\n",
    "            nextdup += 1\n",
    "        i += 1\n",
    "    return nextdup"
   ]
  },
